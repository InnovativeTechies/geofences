<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Acr.Geofencing</id>
        <version>1.0.0-beta1</version>
        <title>ACR Geofencing Plugin for Xamarin and Windows</title>
        <description>Geofencing Plugin for iOS, Android, and Windows</description>
        <authors>Allan Ritchie</authors>
        <owners>Allan Ritchie</owners>
        <licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
        <projectUrl>https://github.com/aritchie/geofences</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <tags>geofences windows xamarin xam.pcl</tags>
        <releaseNotes>
Initial Release
        </releaseNotes>
        <dependencies>
            <group>
                <dependency id="Rx-Main" version="[2.2.5,)" />
            </group>
            <group targetFramework="MonoAndroid10">
                <dependency id="Acr.Settings" version="[5.1.1, 6)" />
                <dependency id="Xam.Plugin.Geolocator" version="[3.0.4, 4)" />
                <dependency id="Rx-Main" version="[2.2.5,)" />
            </group>
            <group targetFramework="uap10.0">
                <dependency id="Acr.Settings" version="[5.1.1, 6)" />
                <dependency id="Xam.Plugin.Geolocator" version="[3.0.4, 4)" />
                <dependency id="Rx-Main" version="[2.2.5,)" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <!-- pcl -->
        <file src="..\src\Acr.Geofencing\bin\Release\Acr.Geofencing.*" target="lib\portable-win+net45+wp8+win8+wpa81" />

        <!-- android -->
        <file src="..\src\Acr.Geofencing.Android\bin\Release\Acr.Geofencing.*" target="lib\MonoAndroid10" />

        <!-- ios -->
        <file src="..\src\Acr.Geofencing.iOS\bin\iPhone\Release\Acr.Geofencing.*" target="lib\Xamarin.iOS10" />

        <!-- uwp -->
        <file src="..\src\Acr.Geofencing.Windows\bin\Release\Acr.Geofencing.*" target="lib\uap10.0" />

    </files>
</package>
